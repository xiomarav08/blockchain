<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<navigation-rule>
		<from-view-id>/adminMenu/usuarios.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>historialContratos</from-outcome>
			<to-view-id>/adminMenu/historialContratos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/adminMenu/historialContratos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>usuarios</from-outcome>
			<to-view-id>/adminMenu/usuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>historialContratos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>salir</from-outcome>
			<to-view-id>login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>plantar.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>payment</from-outcome>
			<to-view-id>paymentMenu/payment.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>plantar.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>billetera</from-outcome>
			<to-view-id>billetera.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>

